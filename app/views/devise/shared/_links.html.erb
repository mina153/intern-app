<% if controller_name != 'sessions' %>
  <%= link_to "Login ", new_session_path(resource_name), class: 'Button' %>
  <br/>
<% end %>
<% if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "New registration", new_registration_path(resource_name), class: 'Button' %>
  <br/>
<% end %>